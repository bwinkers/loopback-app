{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "izzup",
    "public": true,
    "options": {
      "emailVerificationRequired": true,
      "restrictResetPasswordTokenScope": true
    }
  },
  "AccessToken": {
    "dataSource": "izzup",
    "public": false
  },
  "ACL": {
    "dataSource": "izzup",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "izzup",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "izzup",
    "public": false
  },
  "ContactCard": {
    "dataSource": "izzup",
    "public": true
  },
  "Product": {
    "dataSource": "izzup",
    "public": true
  },
  "Email": {
    "dataSource": "mailout",
    "public": false
  }
}
